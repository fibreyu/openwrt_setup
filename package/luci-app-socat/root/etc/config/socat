
config global 'global'
	option enable '1'

config instance 'example'
	option firewall_accept '1'
	option enable '1'
	option remarks 'http'
	option service_type 'port_forwarding'
	option src_proto 'TCP4'
	option src_port '88'
	option reuseaddr '1'
	option dest_proto 'TCP4'
	option dest_port '88'
	option dest_ip '10.0.4.1'


