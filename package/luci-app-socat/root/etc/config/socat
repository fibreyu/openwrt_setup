
config global 'global'
	option enable '1'

config instance 'example'
	option firewall_accept '1'
	option enable '1'
	option remarks 'http'
	option service_type 'port_forwarding'
	option src_proto 'TCP4'
	option src_port '88'
	option reuseaddr '1'
	option dest_proto 'TCP4'
	option dest_port '88'
	option dest_ip '10.0.4.1'

config instance 'http'
	option firewall_accept '1'
	option remarks 'http'
	option src_port '99'
	option reuseaddr '1'
	option dest_proto 'TCP4'
	option dest_port '99'
	option dest_ip '10.0.4.1'
	option service_type 'port_forwarding'
	option src_proto 'TCP4'
	option enable '1'

config instance '6db71006b4947b4213b8f11b064a15aa'
	option enable '1'
	option remarks 'fs'
	option service_type 'port_forwarding'
	option src_proto 'TCP6'
	option src_port '11'
	option reuseaddr '1'
	option dest_proto 'TCP4'
	option dest_ip '10.0.0.13'
	option dest_port '11'
	option firewall_accept '1'

config instance '7a6bf51cc634dc4af0c9ebac8d3822b0'
	option enable '1'
	option remarks 'rrr'
	option service_type 'port_forwarding'
	option src_proto 'TCP6'
	option src_port '22'
	option reuseaddr '1'
	option dest_proto 'TCP4'
	option dest_ip '10.0.0.13'
	option dest_port '22'
	option firewall_accept '1'

